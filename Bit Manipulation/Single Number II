class Solution {
public:
    int singleNumber(vector<int>& nums) {
        int n=nums.size();
        int res=0;
        for(int k=0;k<32;k++){
            int temp=(1<<k);

            int countone=0;
            for(int num:nums){
                if((num & temp)!=0){
                    countone++;
                }
            }
            if(countone%3 == 1){
                res=(res|temp);
            }
        }
        return res;
    }
};
