class Solution {
public:
    int maxSubArray(vector<int>& nums) {
        int max_i=nums[0];
        int sum=0;
        for(int i=0;i<nums.size();i++){
            sum=sum+nums[i];
             max_i=max(max_i,sum);
            if(sum<0){
                sum=0;
            }
        }
        return max_i;
    }
};
