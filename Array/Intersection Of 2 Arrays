class Solution {
public:
    vector<int> intersection(vector<int>& nums1, vector<int>& nums2) {
        vector<int>ans;
        unordered_map<int,int>map;
        for(int i=0;i<nums1.size();i++){
            map[nums1[i]]++;
        }
        for(int i=0;i<nums2.size();i++){
            int num=nums2[i];
            if(map.find(num)!=map.end()){
                ans.push_back(num);
                map.erase(num);
            }
        }
        return ans;
    }
};
