class Solution {
public:
    vector<int> sortArray(vector<int>& nums) {
        int n=nums.size();
        priority_queue<int,vector<int>,greater<int>>minheap;
        for(int i=0;i<n;i++){
            minheap.push(nums[i]);
        }
        for(int i=0;i<n;i++){
            nums[i]=minheap.top();
            minheap.pop();
        }
        return nums;
    }
};
