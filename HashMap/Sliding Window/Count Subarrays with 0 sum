class Solution {
  public:
    int findSubarray(vector<int> &arr) {
        int n=arr.size();
        unordered_map<int,int>mp;
        int pre=0;
        int cnt=0;
        mp[0]++;
        for(int i=0;i<n;i++){
            pre+=arr[i];
            if(mp.find(pre)!=mp.end()){
                cnt+=mp[pre];
            }
            
            mp[pre]++;
        }
        return cnt;
    }
};
